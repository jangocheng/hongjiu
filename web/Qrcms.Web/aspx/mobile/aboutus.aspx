	templateBuilder.Append("/css/style.css\" rel=\"stylesheet\" />\r\n		<title>关于我们</title>\r\n		<style type=\"text/css\">\r\n			body{\r\n				font-size: 12px;\r\n				background: #F2F2F2;\r\n			}\r\n			.wenzhang h3{\r\n				margin-bottom: 15px;\r\n				font-size: 18px;\r\n				font-weight: normal;\r\n			}\r\n			.wenzhang{\r\n				padding: 15px;\r\n			}\r\n			.wenzhang h4{\r\n				font-size: 16px;\r\n				line-height: 24px;\r\n				font-weight: normal;\r\n				\r\n			}\r\n			.wenzhang p{\r\n				line-height: 24px;\r\n				color: #333;\r\n			}\r\n			.mui-content{\r\n				background: #F4F4F4 !important;\r\n			}\r\n			.wenzhang h2{\r\n				font-size: 18px;\r\n				text-align: center;\r\n				font-weight: normal;\r\n				color: #7D0000;\r\n				\r\n			}\r\n			.wenzhang .ab_title{\r\n				text-align: center;\r\n			}\r\n			.wenzhang img{\r\nwidth:100%;\r\n}\r\n		</style>\r\n	</head>\r\n	<body>\r\n		<div class=\"mui-content wenzhang\">\r\n			<p style=\"text-align:center\">\r\n    <strong><span style=\"font-family: 宋体;color: rgb(192, 0, 0);font-size: 24px\"><span style=\"font-family:宋体\">玖方葡萄酒商城</span></span></strong>\r\n</p>\r\n<p style=\"text-indent: 32px\">\r\n    <strong><span style=\"font-family: 宋体;font-size: 16px\"><span style=\"font-family:宋体\">玖方成立于</span>2011年，整体投资2000多万元人民币于葡萄酒事业上，现葡萄酒直营店有十几家，加盟合作商有上百家，玖方一直致力于互联网+葡萄酒的运营模式；</span></strong>\r\n</p>\r\n<p>\r\n    <strong><span style=\"font-family: 宋体;font-size: 16px\"><span style=\"font-family:宋体\">玖方商城是一家专注于葡萄酒文化</span>+旅游+趣味的互联网平台运营商。</span></strong>\r\n</p>\r\n<p>\r\n    <img  src=\"/images/aboutus/1.png\"/><img  src=\"/images/aboutus/2.png\">\r\n</p>\r\n\r\n<p style=\"text-align:center\">\r\n    <strong><span style=\"font-family: 宋体;color: rgb(192, 0, 0);font-size: 21px\"><span style=\"font-family:宋体\">我们的与众不同</span></span></strong>\r\n</p>\r\n<p style=\"text-indent: 32px\">\r\n    <strong><span style=\"font-family: 宋体;font-size: 16px\">玖方葡萄酒商城与传统电子商城不同，玖方商城在酒业内率先融入了猜奇偶、拆红包等系列趣味的购物模式，其主要内涵是客户在购买商品后保值的基础上，有机会体验商品翻倍带来的刺激感受，平台既保证了客户利益不受损，又将客户购物乐趣最大化，让客户得到实惠以及购物乐趣。</span></strong>\r\n</p>\r\n<p>\r\n    <img src=\"/images/aboutus/3.png\"/><img  src=\"/images/aboutus/4.png\"/>\r\n</p>\r\n<p style=\"text-align:center\">\r\n    <strong><span style=\"font-family: 宋体;color: rgb(192, 0, 0);font-size: 21px\"><span style=\"font-family:宋体\">我们的合作伙伴</span></span></strong>\r\n</p>\r\n<p style=\"text-indent: 32px\">\r\n    <strong><span style=\"font-family: 宋体;font-size: 16px\">目前玖方葡萄酒商学院已经和逸香国际葡萄酒教育成立专业</span></strong><strong><span style=\"font-family: 宋体;font-size: 16px\">百人</span></strong><strong><span style=\"font-family: 宋体;font-size: 16px\">讲师团，我们</span></strong><strong><span style=\"font-family: 宋体;letter-spacing: 0;font-size: 16px;background: rgb(255, 255, 255)\"><span style=\"font-family:宋体\">不仅仅传播葡萄酒知识，更要帮助让大家通过</span></span></strong><strong><span style=\"font-family: 宋体;letter-spacing: 0;font-size: 16px;background: rgb(255, 255, 255)\"><span style=\"font-family:宋体\">玖方</span></span></strong><strong><span style=\"font-family: 宋体;letter-spacing: 0;font-size: 16px;background: rgb(255, 255, 255)\"><span style=\"font-family:宋体\">这样广阔的平台，了解更多的葡萄酒文化</span></span></strong><strong><span style=\"font-family: 宋体;letter-spacing: 0;font-size: 16px;background: rgb(255, 255, 255)\"><span style=\"font-family:宋体\">；</span></span></strong>\r\n</p>\r\n<p>\r\n    <img src=\"/images/aboutus/5.png\"/><strong><span style=\"font-family: 宋体;font-size: 12px\">&nbsp;</span></strong>\r\n</p>\r\n<h1 style=\"margin: 4px 14px 0;text-indent: 0;padding: 0;line-height: 36px\">\r\n    <strong><span style=\"font-family: 宋体;color: rgb(192, 0, 0);letter-spacing: 0;font-size: 16px\"><span style=\"font-family:宋体\">认证培训简介</span></span></strong>\r\n</h1>\r\n<p style=\"margin-top:0;margin-right:14px;margin-bottom:0;margin-left:14px;text-indent:32px;padding:0 0 0 0 ;line-height:22px\">\r\n    <strong><span style=\"font-family: 宋体;letter-spacing: 0;font-size: 16px\"><span style=\"font-family:宋体\">逸香国际</span>——全球学员突破15万学员的葡萄酒教育机构。率先在国内引入全球精品葡萄酒课程，是英国WSET葡萄酒与烈酒课程的中国引入者即。此外逸香国际还独立开发了结合国际、国内葡萄酒知识、符合中国人思维习惯，标准的中文版国际葡萄酒品酒师培训认证课程——ESW，2014年逸香又引入ISA侍酒师课程，填补侍酒教育市场，未来逸香还将继续引进国际前沿的葡萄酒课程，为中国葡萄酒爱好者及专业人士服务。</span></strong>\r\n</p>\r\n<p style=\"margin-top:0;margin-right:14px;margin-bottom:0;margin-left:14px;text-indent:32px;padding:0 0 0 0 ;line-height:22px\">\r\n    <strong><span style=\"font-family: 宋体;letter-spacing: 0;font-size: 16px\"><span style=\"font-family:宋体\">逸香是国内较早提供中英文授课的培训机构，课程使用精品进口葡萄酒及教学器具，专业知识和实践品尝使学员的品酒技能快速提升，特别是专门的课后复习使考试通过率可达</span>90％以上。每年在逸香国际学习的学员已过万人，遍布于各行各业、全国各地，许多学员已成为国内星级酒店、大型葡萄酒公司和葡萄酒行业的中坚力量。逸香国际的培训中心现已遍布全国17大城市，培训合作伙伴多达50个城市，可为学员提供便捷的培训服务。</span></strong>\r\n</p>\r\n<p style=\"margin-top:0;margin-right:14px;margin-bottom:0;margin-left:14px;padding:0 0 0 0 ;line-height:22px;display: none;\">\r\n    <strong style=\"line-height: 30px;\r\n    display: inline-flex;\"><span style=\"font-family: 宋体;color: rgb(255, 255, 255);letter-spacing: 0;font-size: 14px;background: rgb(128, 0, 0)\"><span style=\"font-family:宋体\">报名电话：</span></span></strong>\r\n<strong style=\"line-height: 30px;\r\n    display: inline-flex;\"><span style=\"font-family: 微软雅黑;color: rgb(255, 255, 255);letter-spacing: 0;font-size: 14px;background: rgb(128, 0, 0)\">18107715555 </span></strong>\r\n<strong style=\"line-height: 30px;\r\n    display: inline-flex;\"><span style=\"font-family: 宋体;color: rgb(255, 255, 255);letter-spacing: 0;font-size: 14px;background: rgb(128, 0, 0)\"><span style=\"font-family:宋体\">（微信同步）</span></span></strong>\r\n</p>\r\n<p style=\"text-indent: 16px\">\r\n    <a href=\"http://www.eswine.com/renzheng/esw1\"><strong><span style=\"font-family: 宋体;color: rgb(192, 0, 0);letter-spacing: 0;font-size: 16px;background: rgb(255, 255, 255)\">ESW国际品酒师认证课程</span></strong></a>\r\n</p>\r\n<p>\r\n    <img  src=\"/images/aboutus/6.png\"/>\r\n</p>\r\n<p>\r\n    <strong><span style=\"font-family: 宋体;color: rgb(192, 0, 0);letter-spacing: 0;font-size: 16px;background: rgb(255, 255, 255)\">&nbsp;</span></strong>\r\n</p>\r\n<